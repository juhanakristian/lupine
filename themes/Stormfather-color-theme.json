{
  "name": "Stormfather",
  "type": "dark",
  "colors": {
    "focusBorder": "#8695b777",

    // Text
    "textLink.activeForeground": "#8076C2",
    "textLink.foreground": "#8076C2",

    // Editor
    "editor.background": "#1B1929",
    "editor.foreground": "#a2aabc",
    "editor.lineHighlightBackground": "#8076C222",
    "editorCursor.foreground": "#988de4",

    // Activity bar
    "activityBar.background": "#13111d",
    "activityBar.inactiveForeground": "#323b50",
    "activityBar.foreground": "#5c6e92",
    "activityBarBadge.background": "#8076C2",

    // Sidebar
    "sideBar.background": "#1B1929",
    "sideBarTitle.foreground": "#bbbbbb",
    "sideBarSectionHeader.background": "#242c3f",

    // File list
    "list.activeSelectionBackground": "#2a273f",
    "list.activeSelectionForeground": "#8076C2",
    "list.inactiveSelectionBackground": "#2a273f",
    "list.inactiveSelectionForeground": "#8076C2",
    "list.highlightForeground": "#8076C2",
    "list.hoverBackground": "#1f2636",

    // Tabs
    "tab.activeBorder": "#8076C2",
    "tab.activeBackground": "#1d2433",
    "tab.unfocusedActiveBackground": "#1d2433",
    "tab.activeForeground": "#d7dce2",
    "tab.border": "#1B1929",

    // Button
    "button.background": "#8076C2",
    "button.hoverBackground": "#8076C299",

    // Panel colors
    "panel.background": "#1B1929",
    "panel.border": "#1d2433",

    // Status bar
    "statusBar.border": "#1B1929",
    "statusBar.background": "#13111d",
    "statusBar.debuggingBackground": "#ff0000bd",

    // Quick input
    "quickInput.background": "#13111d",

    // cmd+shft+p recently used
    "pickerGroup.foreground": "#8076C2",

    // Editor widget
    "editorWidget.background": "#13111d",
    "editorWidget.border": "#1B1929",

    // input control
    "input.background": "#1e1b2c",

    "titleBar.activeBackground": "#13111d",
    "titleBar.inactiveBackground": "#13111d",
    "titleBar.inactiveForeground": "#5c6e92",

    // "debugToolBar.background": "#1B1929"
    "debugToolBar.background": "#27243b",
    "dropdown.background": "#27243b"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#637777"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#aaa"
      }
    },

    {
      "name": "Object Literal Key",
      "scope": ["meta.object-literal.key.ts"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Builtin classes",
      "scope": ["support.class.builtin.ts"],
      "settings": {
        "foreground": "#7fdbca"
      }
    },
    {
      "name": "Variable Object",
      "scope": ["variable.other.object.ts"],
      "settings": {
        "foreground": "#7fdbca"
      }
    },
    {
      "name": "Variable Parameter Function",
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#7fdbca",
        "fontStyle": ""
      }
    },
    {
      "name": "Variable Other Readwrite",
      "scope": "variable.other.readwrite.ts",
      "settings": {
        "foreground": "#7fdbca",
        "fontStyle": ""
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean.true.ts",
      "settings": {
        "foreground": "#c792ea",
        "fontStyle": ""
      }
    },
    {
      "name": "Variable Instances",
      "scope": [
        "variable.instance",
        "variable.other.instance",
        "variable.readwrite.instance",
        "variable.other.readwrite.instance"
      ],
      "settings": {
        "foreground": "#baebe2"
      }
    },
    {
      "name": "String Quoted",
      "scope": ["string.quoted", "variable.other.readwrite.js"],
      "settings": {
        "foreground": "#ecc48d"
      }
    },
    {
      "name": "Variable Property Other object property",
      "scope": ["variable.other.object.property", "variable.other.property"],
      "settings": {
        "foreground": "#aaa",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable Property Other object",
      "scope": ["variable.other.object.js"],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#c792ea"
      }
    },
    {
      "name": "Keyword Control Conditional",
      "scope": [
        "keyword.control.conditional.js",
        "keyword.control.conditional.ts",
        "keyword.control.switch.js",
        "keyword.control.switch.ts"
      ],
      "settings": {
        "foreground": "#c792ea",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#82AAFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword, control flow",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#537cd6"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type.ts", "meta.type.annotation.ts"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Function Argument",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#ecc48d"
      }
    }
  ]
}
